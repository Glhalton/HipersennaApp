{
  "expo": {
    "name": "GHSApp",
    "slug": "GHSApp",
    "owner": "glhalton",
    "version": "1.1.3",
    "orientation": "portrait",
    "icon": "./assets/images/hipersenna-light-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 6,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/hipersenna-light-icon-v3.png"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.hipersenna.GHSApp",
      "permissions": [
        "INTERNET"
      ],
      "usesCleartextTraffic": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/hipersenna-light-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/images/hipersenna-red-logo.png",
          "resizeMode": "contain",
          "dark": {
            "backgroundColor": "#232323",
            "image": "./assets/images/hipersenna-red-logo.png"
          },
          "imageWidth": 200
        }
      ],
      "expo-font",
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "GHSApp precisa de acesso a sua c√¢mera.",
          "enableCodeScanner": true
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "eb345444-6520-4922-b8e6-72b61581ed29"
      }
    }
  }
}
