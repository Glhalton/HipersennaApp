{
  "expo": {
    "name": "GHSApp",
    "slug": "GHSApp",
    "owner": "glhalton.hipersenna",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/hipersenna-light-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/hipersenna-light-icon-v2.png"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.hipersenna.GHSApp",
      "permissions": [
        "INTERNET"
      ],
      "usesCleartextTraffic": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/hipersenna-light-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/images/hipersenna-light-splash-icon.png",
          "resizeMode": "contain",
          "dark": {
            "backgroundColor": "#232323",
            "image": "./assets/images/hipersenna-dark-splash-icon.png"
          },
          "imageWidth": 200
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f23da512-ccfc-4e36-a709-0d57b885bdcf"
      }
    }
  }
}
